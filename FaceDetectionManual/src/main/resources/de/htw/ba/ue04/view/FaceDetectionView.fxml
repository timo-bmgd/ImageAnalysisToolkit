<AnchorPane style="-fx-font-family: Arial;" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="200.0" prefWidth="1150.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.htw.ba.ue04.controller.FaceDetectionController">
   <children>
