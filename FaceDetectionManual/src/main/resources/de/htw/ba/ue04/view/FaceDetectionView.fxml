                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </ComboBox>
                  <Label text="Left View" />
                  <ComboBox fx:id="leftViewSelection" prefHeight="27.0" prefWidth="187.0">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin></ComboBox>
