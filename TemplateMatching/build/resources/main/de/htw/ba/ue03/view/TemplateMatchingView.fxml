<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane style="-fx-font-family: Arial;" prefHeight="653.0" prefWidth="877.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.htw.ba.ue03.controller.TemplateMatchingController">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER" prefHeight="25.0" prefWidth="792.0" spacing="10.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#onOpenFileClick" prefHeight="25.0" prefWidth="93.0" text="Bild laden" />
                  <Label text="Methode:">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                  </Label>
                  <ComboBox fx:id="methodSelection" prefWidth="150.0" />
                  <Button mnemonicParsing="false" onAction="#applyTemplateMatching" prefHeight="25.0" prefWidth="110.0" text="Ausführen">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
               <children>
                  <ImageView fx:id="leftImageView" pickOnBounds="true" preserveRatio="true" HBox.hgrow="ALWAYS">
        
                  </ImageView>
                  <ImageView fx:id="rightImageView" pickOnBounds="true" preserveRatio="true" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin></ImageView>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="20.0" prefWidth="200.0">
               <children>
                  <Label fx:id="runtimeLabel" text="Methode X ausgeführt in 0 ms" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
